#include <iostream>
 
using namespace std;
 void code()
 {
     int x;
     cin>>x;
     if(x==0) cout<<"NULL\n";
     else
     {
         if(x<0)
         {
             if(abs(x)%2==0) cout<<"EVEN";
             else cout<<"ODD";
             
             cout<<" NEGATIVE\n";
             
         }
         else
         {
              if(x%2==0) cout<<"EVEN";
             else cout<<"ODD";
             
             cout<<" POSITIVE\n";
             
         }
     }
 }
int main() {
 
    int tt;
    cin>>tt;
    while(tt--) code();
 
    return 0;
}
