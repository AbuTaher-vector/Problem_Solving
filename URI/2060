#include <iostream>
 
using namespace std;
 
int main() {
 
    int a=0,b=0,c=0,d=0;
    int x,t;
    cin>>t;
    while(t--)
    {
        cin>>x;
        if(x%2==0) a++;
        if(x%3==0) b++;
        if(x%4==0) c++;
        if(x%5==0) d++;
    }
    cout<<a<<" Multiplo(s) de 2\n";
     cout<<b<<" Multiplo(s) de 3\n";
      cout<<c<<" Multiplo(s) de 4\n";
       cout<<d<<" Multiplo(s) de 5\n";
 
    return 0;
}
