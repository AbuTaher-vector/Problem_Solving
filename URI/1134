#include <iostream>
 
using namespace std;
 
int main() {
 
   int a=0,b=0,c=0,x;
   while(1)
   {
       cin>>x;
       if(x==1) a++;
       else if(x==2) b++;
       else if(x==3) c++;
       
       if(x==4) break;
   }
    cout<<"MUITO OBRIGADO\n"<<"Alcool: "<<a<<"\n"<<"Gasolina: "<<b<<'\n'<<"Diesel: "<<c<<'\n';
    return 0;
}
