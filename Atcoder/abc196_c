#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl '\n'
int main()
{
     ll n;
     cin>>n;
     ll cnt=0;
     for(nn i = 1; i <=999999;i++)
     {
 
         ss s = to_string(i);
         ss s2 = s;
 
         ll k = stoll(s+s2);
 
         if(k<=n) cnt++;
 
     }
     cout<<cnt<< endl;
    
}
