#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int 
#define ss string
#define nl "\n"
int main()
{ 
    nn n;
    cin>>n;
    vector<nn>v(n);
    ll xr=0;
    for(nn i=0;i<n;i++) 
    {
         cin>>v[i];
         xr^=v[i];
    }
     for(nn i=0;i<n;i++) 
    {
         cout<<(xr^v[i])<<" ";
    }
    cout<<nl;
   
   
}
