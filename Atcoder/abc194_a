#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{
    nn a,b;
    cin>>a>>b;
    nn k=a+b;
    if(k>=15 && b>=8) cout<<1<<endl;
    else if(k>=10 && b>=3) cout<<2<<endl;
    else if(k>=3) cout<<3<<endl;
    else if(a==0 || b==0) cout<<4<<endl;
 
}
