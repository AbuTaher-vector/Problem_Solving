#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{
	ss s;
	cin>>s;
	reverse(s.begin(),s.end());
	for(nn i=0;i<s.size();i++)
	{
		if(s[i]=='6') s[i]='9';
		else if(s[i]=='9') s[i]='6';
	}
	cout<<s<<"\n";
 
 
}
