class Solution {
public:
    int countPrimeSetBits(int l, int r) {
         int cnt=0;
     for(int i=l;i<=r;i++)
     {
        int k=__builtin_popcount(i);
        int flag=1;
        if(k!=1)
        {
            for(int j=2;j<=sqrt(k);j++)
        {
            if(k%j==0)
            {
                flag=0;
                break;
            }

        }
        if(flag) cnt++;

        }

     }
     return cnt;
        
    }
};
