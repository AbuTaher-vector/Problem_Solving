#include <bits/stdc++.h>
using namespace std;

int main() {
	char ch[201];
	cin>>ch;
	int i,flag=0;
	for(i=0;i<strlen(ch);i++)
    {
        if(ch[i]=='W' && ch[i+1]=='U' && ch[i+2]=='B')
        {
            i+=2;
            if(flag) cout<<" ";
        }
        else
        {
            flag=1;
            cout<<ch[i];
        }
    }
    cout<<endl;
}
