#include<bits/stdc++.h>
using namespace std;
int main()
{
    char a[100],b[100];
    cin>>a>>b;
    int c=0;
    for(int i=0,j=strlen(b)-1;i<strlen(a),j>=0;i++,j--)
    {
        if(a[i]!=b[j])
        {c++;
        break;
        }
    }
    if(c==0) cout<<"YES"<<endl;
     else cout<<"NO"<<endl;
}
