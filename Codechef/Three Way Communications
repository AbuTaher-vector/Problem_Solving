#include <bits/stdc++.h>
using namespace std;

int main()
{
	double t,r,a,b,c,d,e,f;
	cin>>t;
	while(t--)
	{
	     cin>>r;
	     cin>>a>>b;
	     cin>>c>>d;
	     cin>>e>>f;
	     double z=sqrt(pow((a-e),2)+pow((b-f),2));

	     double x=sqrt(pow((a-c),2)+pow((b-d),2));

	     double y=sqrt(pow((c-e),2)+pow((d-f),2));

	     if(z<=r)
	          cout<<"yes"<<endl;
	     else if(x<=r&&y<=r)
	          cout<<"yes"<<endl;
	     else
	          cout<<"no"<<endl;
	}
	return 0;
}
