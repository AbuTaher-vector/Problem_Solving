#include<bits/stdc++.h>
using namespace std;
int main()
{
  int tt;
  cin>>tt;
  while(tt--)
  {
      int t=2048,cnt=0;
      int x;
      cin>>x;
      while(x!=0)
      {
          while(t<=x)
          {
              x=x-t;
              cnt++;
          }
          t/=2;
      }
      cout<<cnt<<endl;
  }
}
