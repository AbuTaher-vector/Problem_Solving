#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{

    nn tt;
    cin>>tt;
    while(tt--)
    {
        nn n ,k;
        cin>>n>>k;
        nn arr[n+1];

        for(nn i=0;i<n;i++)  cin>>arr[i];
        for(nn i=0;i<n;i++)
        {
            if(k>=arr[i])
            {
                cout<<1;
                k-=arr[i];
            }
            else cout<<0;

        }
        cout<<endl;
    }


}
