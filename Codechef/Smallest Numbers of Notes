#include<bits/stdc++.h>
using namespace std;
int main()
{
     int tt;
     cin>>tt;
     while(tt--)
     {
         int x,sum=0;
         cin>>x;
         sum+=x/100;
         x%=100;
         sum+=x/50;
         x%=50;
          sum+=x/10;
          x%=10;
          sum+=x/5;
          x%=5;
          sum+=x/2;
          x%=2;
          sum+=x;
          cout<<sum<<endl;


     }
}
