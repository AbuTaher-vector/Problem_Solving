#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl '\n'
void setup()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
     #ifndef ONLINE_JUDGE
     freopen("input.txt", "r", stdin);
     freopen("outputt.txt", "w", stdout);
     #endif

}
void code()
{
    nn n;
    cin>>n;
    nn x,cnt=0;
    while(n--)
    {
        cin>>x;
        if(x>=1000) cnt++;
    }
    cout<<cnt<<nl;
}
int main()
{
        setup();
        nn tt;
        cin>>tt;
        while(tt--) code();
}
