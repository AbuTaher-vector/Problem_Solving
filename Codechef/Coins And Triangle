#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{
    nn tt;
    cin>>tt;
    while(tt--)
    {
        nn x;
        cin>>x;
        nn i=1,cnt=0;
        while(x>=i)
        {
           cnt++;
           x-=i;
           i++;
        }
        cout<<cnt<<endl;
    }

}

