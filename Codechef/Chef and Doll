#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{

   nn tt;
   cin>>tt;
   while(tt--)
   {
       nn x;
       cin>>x;
       nn arr[x+1],cnt[100001]={0};
       for(nn i=0;i<x;i++)
       {
           cin>>arr[i];
           cnt[arr[i]]++;
       }
       for(nn i=0;i<100001;i++)
       {
           if(cnt[i]%2!=0)
           {
               cout<<i<<endl;
           break;
           }
       }

   }


}
