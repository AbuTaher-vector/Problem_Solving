#include<bits/stdc++.h>
using namespace std;

int main()
{
    int test,h,t;
    float car;
    cin>>test;

    while(test--)
    {
        cin>>h>>car>>t;

        if(h>50 && car<0.7 && t>5600)
            cout<< "10"<<endl;
        else if(h>50 && car<0.7)
            cout<< "9"<<endl;
        else if(car<0.7 && t>5600)
            cout<< "8"<<endl;
        else if(h>50 && t>5600)
            cout<< "7"<<endl;
        else if(h>50 || car<0.7 || t>5600)
            cout<< "6"<<endl;
        else
            cout<< "5"<<endl;
    }
    return 0;
}
