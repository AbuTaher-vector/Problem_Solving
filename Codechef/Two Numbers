#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{
    ll tt;
    cin>>tt;
    while(tt--)
    {
        ll a,b,n;
        cin>>a>>b>>n;

        if(n%2==0) cout<<max(a,b)/min(a,b)<<endl;
        else cout<<max(a*2,b)/min(a*2,b)<<endl;


    }

}
