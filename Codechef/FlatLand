#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{
     nn tt;
     cin>>tt;
     while(tt--)
     {
         nn t,cnt=0;
         cin>>t;
         while(t!=0)
         {
             nn k=sqrt(t);
             t-=(k*k);
             cnt++;
         }
         cout<<cnt<<endl;
     }


}
