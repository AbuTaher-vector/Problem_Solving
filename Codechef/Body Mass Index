#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{
    nn tt;
    cin>>tt;
    while(tt--)
    {
        nn m,h;
        cin>>m>>h;
        nn k=m/(h*h);
        if(k<=18) cout<<1<<endl;
        else if(k>=19 && k<=24) cout<<2<<endl;
        else if(k>=25 && k<=29) cout<<3<<endl;
        else if(k>=30) cout<<4<<endl;
    }

}
