#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl "\n"
void code()
{
   ss a;
   cin>>a;
   nn sum=0;
   for(nn i=0;i<a.size();i++)
   {
     if(isdigit(a[i]))
     {
      sum+=(a[i]-'0');
     }
   }
   cout<<sum<<nl;

}
int main()
{
  nn tt;
  cin>>tt;
  while(tt--) code();
}
