#include<bits/stdc++.h>
using namespace std;
int main()
{
    int tt;
    cin>>tt;
    while(tt--)
    {
        int x,sum=0;
        cin>>x;
        int arr[x+1];
        for(int i=1;i<=x;i++)
        {
            cin>>arr[i];
           sum+=arr[i];
        }
        if(sum>-0) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;



    }
}
