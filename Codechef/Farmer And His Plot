
#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{

      nn tt;
      cin>>tt;
      while(tt--)
      {
          nn m,n;
          cin>>m>>n;
          nn x=__gcd(m,n);
          cout<<(m*n)/(x*x)<<endl;
      }


}
