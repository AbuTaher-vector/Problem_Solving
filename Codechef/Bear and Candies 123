#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{
     nn tt;
     cin>>tt;
     while(tt--)
     {
         nn a,b;
         cin>>a>>b;
         nn x=0,y=0,flag=0,flag2=0;
         for(nn i=1,j=2;;i+=2,j+=2)
         {
                    x+=i,y+=j;
                    if(x>a)
                    {
                        flag=1;
                        break;
                    }
                    if(y>b)
                    {
                        flag2=1;
                        break;
                    }
         }
         if(flag==0) cout<<"Limak"<<endl;
         if(flag2==0) cout<<"Bob"<<endl;
     }



}
