#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl '\n'
void code()
{
     ll l,r;
     cin>>l>>r;
     if(l%2 && r-l==3) cout<<-1<<nl;
     else
     {
         ll x=l;
         if(x%2) x++;
         cout<<x<<" "<<x+1<<" "<<x+2<<" "<<x+3<<nl;
     }
}
int main()
{
     #ifndef ONLINE_JUDGE
     freopen("input.txt", "r", stdin);
     freopen("outputt.txt", "w", stdout);
     #endif
     nn tt;
     cin>>tt;
     while(tt--) code();
}
