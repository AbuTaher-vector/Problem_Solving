#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
void code()
{
      ll n;
      cin>>n;
      nn arr[n+1];
      map<ll,ll>mp;
      ll ans=0;
      for(ll i=0;i<n;i++) {
          
          cin>>arr[i];
          mp[arr[i]]++;
      } 
      for(auto x : mp) ans+=(n-x.second)*x.second;
        cout<<ans<<"\n";
      
      
      
    
}
int main()
{
  nn tt;
  cin>>tt;
  while(tt--) code(); 
    
}
