#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl '\n'
void code()
{
    nn n;
    cin>>n;
       char x='a';
       for(nn i=1;i<=n;i++)
       {
        if(i%26==1) x='a';
        cout<<x;
        x++;
       }
       cout<<nl;

}
int main()
{
    nn tt;
    cin>>tt;
    while(tt--) code();

   
}
