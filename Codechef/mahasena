#include<bits/stdc++.h>
using namespace std;
int main()
{
    int tt,even=0,odd=0;
    cin>>tt;
    while(tt--)
    {
        int x;
        cin>>x;
        if(x%2==0) even++;
        else odd++;
    }
    if(even>odd) cout<<"READY FOR BATTLE"<<endl;
    else cout<<"NOT READY"<<endl;
}
