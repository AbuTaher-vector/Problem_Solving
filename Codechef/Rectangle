#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{
    nn tt;
    cin>>tt;
    while(tt--)
    {
        nn x,cnt=0;
         nn arr[5];
         for(nn i=0;i<4;i++) cin>>arr[i];
         sort(arr,arr+4);
         for(nn i=0;i<4;i=i+2)
         {
             if(arr[i]==arr[i+1]) cnt++;
         }

        if(cnt==2) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;

    }

}
