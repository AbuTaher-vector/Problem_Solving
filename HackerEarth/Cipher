#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{
   nn tt;
   ss s;
   cin>>s>>tt;
   for(nn i=0;i<s.size();i++)
   {
       if(s[i]>=48 && s[i]<=57)
       {
               nn k=tt%10;
               s[i]+=k;
               if(s[i]>57) s[i]-=10;
       }
        else if(s[i]>=65 && s[i]<=90)
       {
               nn k=tt%26;
               s[i]+=k;
               if(s[i]>90) s[i]-=26;
       }
      else if(s[i]>=97 && s[i]<=122)
       {
                s[i]-=26;
               nn k=tt%26;
               s[i]+=k;
              if(s[i]>96)  s[i]-=26;
              s[i]+=26;
       }
       cout<<s[i];
   }
   cout<<endl;

}
