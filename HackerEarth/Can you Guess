#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{
      nn tt;
      cin>>tt;
      while(tt--)
      {
          nn x,sum=0;
          cin>>x;
          for(nn i=1;i<x;i++)  if(x%i==0) sum+=i;

             cout<<sum<<endl;

      }

}
