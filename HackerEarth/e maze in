#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{

    nn x=0,y=0;
    ss s;
    cin>>s;
    for(nn i=0;i<s.size();i++)
    {
        if(s[i]=='R') x++;
        if(s[i]=='L') x--;
        if(s[i]=='U') y++;
        if(s[i]=='D') y--;

    }
    cout<<x<<" "<<y<<endl;


}
