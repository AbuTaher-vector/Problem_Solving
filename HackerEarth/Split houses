#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{

     nn tt,flag=1;
     cin>>tt;
     ss s;
     cin>>s;
     for(nn i=0;i<s.size();i++)
     {
         if(s[i]=='H' && s[i+1]=='H')
         {
             flag=0;
             break;
         }
         if(s[i]=='.') s[i]='B';
     }
     if(flag==1) cout<<"YES"<<endl<<s<<endl;
    else cout<<"NO"<<endl;
}
