#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
int main()
{

      nn tt,t;
      cin>>tt>>t;
      while(tt--)
      {
          nn x;
          cin>>x;
          if(x>=t) cout<<"YES"<<endl;
          else cout<<"NO"<<endl;
      }

}
