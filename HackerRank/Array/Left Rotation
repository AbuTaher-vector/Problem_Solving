#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl '\n'
int main()
{
    /* #ifndef ONLINE_JUDGE
     freopen("input.txt", "r", stdin);
     freopen("outputt.txt", "w", stdout);
     #endif*/

    nn n,d;
    cin>>n>>d;
    vector<nn>v(n);
    for(nn i=0;i<n;i++) cin>>v[i];
     reverse(v.begin(),v.begin()+d);
    reverse(v.begin()+d,v.end());
    reverse(v.begin(),v.end());
    for(auto x:v) cout<<x<<" ";
     cout<<nl;

}
